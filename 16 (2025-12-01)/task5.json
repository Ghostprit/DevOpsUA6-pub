{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Task 5 Schema",
    "type": "object",
    "properties": {
        "student": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "age": {
                    "type": "integer"
                },
                "grade": {
                    "type": "string"
                },
                "subjects": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": ["name", "age", "grade", "subjects"]
        }
    },
    "required": [
        "student"
    ]
}